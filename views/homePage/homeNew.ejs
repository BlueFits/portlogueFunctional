<section>
    <ul><!-- Custom Limited-->
    <%- (function(qUsers) {
        let newDisplay = [];

        if (qUsers.length <= 10) {
            for (let i = 0; i < qUsers.length; i++) {
                newDisplay.push( `<li><a href="/users/${qUsers[i].username}">
                    <div>
                        <div><!--Grey bar--></div>
                        <div><img src="/userWebThumb/${qUsers[i].email}"/></div>
                    </div></a>
                    <div>
                        <div>
                            <div><img src="/publicAvatar/${qUsers[i].email}"/></div>
                            <a href="/users/${qUsers[i].username}">${qUsers[i].fullNameDisplay}</a>
                        </div>
                        <span>${qUsers[i].occupation}</span>
                    </div>
                </li>`);
            }
            return newDisplay.join('');
        }
        else {
            for (let i = 0; i < 10; i++) {
                newDisplay.push( `<li><a href="/users/${qUsers[i].username}">
                        <div>
                            <div><!--Grey bar--></div>
                            <div><img src="/userWebThumb/${qUsers[i].email}"/></div>
                        </div></a>
                        <div>
                            <div>
                                <a href="/users/${qUsers[i].username}">
                                    <div><img src="/publicAvatar/${qUsers[i].email}"/></div>
                                    <span>${qUsers[i].fullNameDisplay}</span>
                                </a>
                            </div>
                            <span>${qUsers[i].occupation}</span>
                        </div>
                    </li>`);
            }
            return newDisplay.join('');
        }
    })(qUsers.reverse());%>
    </ul>

    <a href="/willBeImplemented">See more</a>
    <!-- Reverse to take advantage of mongo listing-->
</section>